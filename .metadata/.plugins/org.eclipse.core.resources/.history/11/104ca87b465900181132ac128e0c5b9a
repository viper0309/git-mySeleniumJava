public class ArryaTests {
	
	public static void main(String[] args) {
	
		String name="shreya";
		System.out.println("before revers the name ");
		String rev="";
		
		for(int i=0;i>=name.length();i--) {
			rev=rev+name.charAt(i);
			
		System.out.println("after revers the name "+rev);	
		}
		
		
	
	
	}
	


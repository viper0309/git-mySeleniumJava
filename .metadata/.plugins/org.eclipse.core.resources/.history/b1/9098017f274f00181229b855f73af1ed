package com.java.tests;

public class Array {

	public static void main(String[] args) {
int arr[]=new int [3];
arr[0]=30;
arr[1]=10;
arr[2]=20;
for(int i=0;i<arr.length;i++) {
System.out.println(arr[i]);	
}
System.out.println("++++++++++++++++++++++++++++++");
String arr1[]= {"alyssa","david","shreya"};
for(int i=0;i<arr1.length;i++) {
System.out.println(arr1[i]);	
}

	}

}

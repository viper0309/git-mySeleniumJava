package selenium;

public class Test {//Encapsulation encapsulate everything into one un
	
	public static void main(String[] args) {
	
		Test obj=new Test();
		
		obj.add(10, 20);
		obj.add("shreya ", 30);
		obj.add(10,20,30);
		
	}
		
	public void add(int a, int b) {
		int c=a+b;
		System.out.println("two aruguments method "+c);
	}
	public void add(int a, int b,int d) {
		int c=a+b+d;
		System.out.println("three aruguments method "+c);	
	}
		
	public void add(String a, int b) {
		String c=a+b;
		System.out.println("two aruguments String method "+c);	
		
	}
}
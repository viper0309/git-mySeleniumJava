package com.java.tests;

public class PolyMorphism {

	
	public static void add(int a, int b) {
		int c=a+b;
		System.out.println("Add method with two para value is "+c);
	}
	public static void add(int a, int b, int c) {
		int d=a+b+c;
		System.out.println("Add method with three para value is "+d);
	}
	
	
	public static void main(String[] args) {
add(100,100);
add(30,30,30);


String name="shreya yum interview";
String [] spitNme=name.split(" ");
for(int i=0;i<spitNme.length;i++) {
	System.out.println(spitNme[i]);
	
}

	}

}
